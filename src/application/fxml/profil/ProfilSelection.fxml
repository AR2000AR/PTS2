<AnchorPane prefHeight="720.0" prefWidth="1080.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ProfilSelectionController">
   <children>
      <ImageView fitHeight="720.0" fitWidth="1080.0" pickOnBounds="true">
         <image>
            <Image url="@../../../image/fond.png" />
         </image>
         <rotationAxis>
            <Point3D />
         </rotationAxis>
      </ImageView>
      <BorderPane prefHeight="720.0" prefWidth="1080.0">
         <top>
            <Label text="Les 3 Petits Cochons" BorderPane.alignment="CENTER">
               <font>
                  <Font name="Lucida Calligraphy Italic" size="60.0" />
               </font>
            </Label>
         </top>
         <center>
            <VBox fx:id="vBox" alignment="CENTER">
               <padding>
                  <Insets left="30.0" right="30.0" />
               </padding>
            </VBox>
         </center>
      </BorderPane>
   </children>
   <rotationAxis>
      <Point3D />
   </rotationAxis>
</AnchorPane>
